<h1>New tropy</h1>

<% form_for(@tropy) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :pageid %><br />
    <%= f.text_field :pageid %>
  </p>
  <p>
    <%= f.label :content %><br />
    <%= f.text_area :content %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', tropies_path %>